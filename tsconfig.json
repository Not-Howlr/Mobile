{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"target": "ESNext",
		"jsx": "react",
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"strict": true
	}
}